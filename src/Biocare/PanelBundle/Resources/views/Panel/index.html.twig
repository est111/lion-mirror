{% extends '::base.html.twig' %}

{% block body -%}
    <div class='container'>
        <div class='row'>
            <div class='col-lg-3'>
                <div class="panel panel-danger">
                    <div class="panel-heading">
                        <strong>Debug</strong>
                    </div>
                    <div class="panel-body">
                        It's all about call at:<br/> <strong>[{{ callregister.id}}]{{ callregister.created|date('Y-m-d H:i') }}</strong>
                        <br/><br/>
                        Hello <strong>{{ callregister.createdBy }}</strong>
                        <br/><br/>
                        You are logged from <strong>{{ callregister.createdFromIp }}</strong>
                        <br/><br/>
                        Call source: <strong>{{ callregister.source }}</strong>
                        <br/><br/>
                        You are representing: <strong>{{ callregister.destination }}</strong>
                    </div>
                </div>
            </div>
            <div class='col-lg-9'>
                {{ render(url('customer')) }}
            </div>
        </div>
    </div>
{% endblock %}
