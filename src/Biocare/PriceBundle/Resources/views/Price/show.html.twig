{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Price</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Startat</th>
                <td>{{ entity.startAt|date('Y-m-d H:i') }}</td>
            </tr>
            <tr>
                <th>Currency</th>
                <td>{{ entity.currency }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_price') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_price_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
