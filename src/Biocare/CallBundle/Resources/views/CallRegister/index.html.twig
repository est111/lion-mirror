{% extends '::base.html.twig' %}

{% block body -%}
    <h1>CallRegister list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Createdfromip</th>
                <th>Created</th>
                <th>Createdby</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('admin_callregister__show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.createdFromIp }}</td>
                <td>{{ entity.created }}</td>
                <td>{{ entity.createdBy }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('admin_callregister__show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('admin_callregister__edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('admin_callregister__new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
